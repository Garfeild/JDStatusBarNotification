ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FTFontSelector" "${PODS_ROOT}/Headers/Public/InfColorPicker"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/FTFontSelector" "$PODS_CONFIGURATION_BUILD_DIR/InfColorPicker"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FTFontSelector" -isystem "${PODS_ROOT}/Headers/Public/InfColorPicker"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FTFontSelector" -l"InfColorPicker" -framework "CoreText"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
